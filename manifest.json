{
  "manifest_version": 3,
  "name": "DeDupe2Activate",
  "version": "1.0",
  "author": "Webber",
  "description": "Automatically closes duplicate tabs, Activate Remaining Tab",
  "permissions": [
    "tabs",
    "webNavigation",
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "incognito": "split",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
